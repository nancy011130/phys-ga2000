import numpy as np
import matplotlib.pyplot as plt
from scipy.stats import norm
print("Hello world")
x = np.arange(-10, 10, 0.01)

mean = 0
std = 3

y = 1/(std*np.sqrt(2*np.pi)) * (np.exp(-0.5*((x-mean)/std)**2))
plt.plot(x, y)
plt.show